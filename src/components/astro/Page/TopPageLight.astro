---
import Excerpt from "@components/astro/Base/Excerpt.astro";
import Separator from "@components/astro/Base/Separator.astro";

const { frontmatter } = Astro.props;
---

<div class="container mt-40 flex flex-col gap-y-2">
    <h1>{frontmatter.title}</h1>
    <Excerpt content={frontmatter.excerpt} />
    <Separator styles="my-10" />
</div>
